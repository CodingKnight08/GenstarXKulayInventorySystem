<MudDialog>
    <DialogContent>
        
        <MudText Typo="Typo.h6" GutterBottom="true" Align="Align.Center">Delete Daily Sale Report</MudText>
        <MudText Typo="Typo.body1" GutterBottom="true" Align="Align.Center">Are you sure you want to delete this daily sale report?</MudText>
        <MudText Typo="Typo.body2" Color="Color.Error" GutterBottom="true" Align="Align.Center">This action cannot be undone.</MudText>
     
    </DialogContent>
    <DialogActions>
        <MudButton Color="Color.Default" Variant="Variant.Filled" OnClick="Cancel">Cancel</MudButton>
        <MudButton StartIcon="@Icons.Material.Filled.Delete" Color="Color.Error" Variant="Variant.Filled" OnClick="DeleteDailySaleReportAsync">Delete</MudButton>
    </DialogActions>
</MudDialog>